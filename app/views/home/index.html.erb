<h2>Statuses for NW6</h2>


<%= will_paginate @entries, :renderer => BootstrapPagination::Rails %>
<table class="table table-striped">
	<thead>
		<tr>
			<th>Time</th>
			<th>Status</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @entries.each do |entry| %>
		<tr>
			<td><%= entry.created_at %></td>
			<td><%= entry.parsed_status %></td>
			<td><a href="/view_response?id=<%= entry.id %>">View</a></td>
		</tr>
		<% end %>
	</tbody>
</table>
<%= will_paginate @entries, :renderer => BootstrapPagination::Rails %>